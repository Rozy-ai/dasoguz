{
  "name": "yiisoft/yii2-app-advanced",
  "description": "Yii 2 Advanced Project Template",
  "keywords": [
    "yii2",
    "framework",
    "advanced",
    "project template"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0.0",
    "yiisoft/yii2": "2.0.16.1",
    "yiisoft/yii2-bootstrap4": "~2.0.6",
    "yiisoft/yii2-swiftmailer": "*",
    "dektrium/yii2-user": "dev-master",
    "dektrium/yii2-rbac": "dev-master",
    "kartik-v/yii2-widget-datepicker": "@dev",
    "kartik-v/yii2-field-range": "*",
    "zyx/zyx-phpmailer": "dev-master",
    "perminder-klair/yii2-dropzone": "v1.0.1",
    "yurkinx/yii2-image": "v1.1",
    "philippfrenzel/yii2fullcalendar":"3.9.0",
    "2amigos/yii2-ckeditor-widget" : "~2.1",
    "iutbay/yii2-kcfinder" : "dev-master",
    "yiisoft/yii2-jui": "^2.0",
    "bilginnet/yii2-cropper": "dev-master",
    "beastbytes/yii2-wizard": "v1.0.0",
    "kartik-v/yii2-widget-activeform": "v1.5.8",
    "kartik-v/yii2-widgets": "3.4.0.x-dev",
    "kartik-v/yii2-helpers": "1.3.x-dev",
    "kartik-v/yii2-slider": "dev-master",
    "evgeniyrru/yii2-slick" : "*",
    "2amigos/yii2-gallery-widget": "1.1.0",
    "yiisoft/yii2-authclient": "2.1.3",
    "bookin/yii2-awesome-bootstrap-checkbox": "dev-master",
    "rsr/yii2-button-dropdown-sorter": "*",
    "mbrowniebytes/yii2-clean-vendors": "dev-master",
    "codemix/yii2-localeurls": "1.5.0",
    "omgdef/yii2-multilingual-behavior": "2.1.2",
    "open-ecommerce/yii2-backuprestore": "v1.0",
    "skeeks/yii2-mobile-detect": "1.0.1",
    "arogachev/yii2-many-to-many": "0.3.0",
    "himiklab/yii2-recaptcha-widget": "1.2.2",
    "2amigos/qrcode-library": "1.1.2",
    "kartik-v/yii2-mpdf": "dev-master",
    "sjaakp/yii2-taggable": "2.0.2",
    "demi/sitemap-generator": "~1.0"
	
  },
  "require-dev": {
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*"
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    }
  },
  "scripts": {
    "post-install-cmd": "php init --env=Development --overwrite=n",
    "post-update-cmd": [
      "php yii clean-vendors"
    ]
  }
}
